<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="payment_asynchronous_process" model="ir.cron">
        <field name="name">Enviar transacciones de pago</field>
        <field name="model_id" ref="payment.model_payment_transaction"/>
        <field name="state">code</field>
        <field name="code">model.cron_asynchronous_process()</field>
        <field name="nextcall" eval="DateTime.now().strftime('%Y-01-01 04:00:00')"/>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>
</odoo>
